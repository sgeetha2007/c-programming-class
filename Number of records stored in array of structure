#include <stdio.h>

struct Student {
    int id;
    char name[20];
};

int main() {
    int n, i;
    struct Student s[50];

    printf("Enter number of students: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("\nEnter details of student %d\n", i + 1);
        printf("ID: ");
        scanf("%d", &s[i].id);
        printf("Name: ");
        scanf("%s", s[i].name);
    }

  
    printf("\nTotal number of records stored = %d", n);

    return 0;
}
