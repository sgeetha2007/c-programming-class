#include <stdio.h>

int main() {
    int a[50], n, add, i;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    printf("How many new records to add? ");
    scanf("%d", &add);

    printf("Enter %d new elements:\n", add);
    for(i = n; i < n + add; i++) {
        scanf("%d", &a[i]);
    }

    n = n + add;   
    printf("\nUpdated List:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }

    return 0;
}
