#include <stdio.h>

long long factorial(int n) {
    long long fact = 1;
    int i;
    for(i = 1; i <= n; i++) {
        fact *= i;
    }
    return fact;
}

int main() {
    int num;
    long long fact;

    printf("Enter a number: ");
    scanf("%d", &num);

    if(num < 0)
        printf("Factorial is not defined for negative numbers");
    else {
        fact = factorial(num);
        printf("Factorial of %d = %lld", num, fact);
    }

    return 0;
}
