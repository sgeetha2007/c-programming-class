#include <stdio.h>

struct Customer {
    int id;
    char name[30];
    long long mobile;
};

int main() {
    int n, i, searchId, found = 0;
    struct Customer c[50];

    printf("Enter number of customers: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("\nEnter details of customer %d\n", i + 1);

        printf("Customer ID: ");
        scanf("%d", &c[i].id);

        printf("Name: ");
        scanf("%s", c[i].name);

        printf("Mobile Number: ");
        scanf("%lld", &c[i].mobile);
    }

    printf("\nEnter Customer ID to search: ");
    scanf("%d", &searchId);

    for(i = 0; i < n; i++) {
        if(c[i].id == searchId) {
            printf("\nCustomer Found\n");
            printf("ID: %d\nName: %s\nMobile: %lld",
                   c[i].id, c[i].name, c[i].mobile);
            found = 1;
            break;
        }
    }

    if(found == 0) {
        printf("\nCustomer not found");
    }

    return 0;
}
