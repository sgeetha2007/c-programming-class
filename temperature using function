#include <stdio.h>

float celsiusToFahrenheit(float celsius) {
    return (celsius * 9 / 5) + 32;
}

int main() {
    float c, f;

    printf("Enter temperature in Celsius: ");
    scanf("%f", &c);

    f = celsiusToFahrenheit(c);

    printf("Temperature in Fahrenheit = %.2f", f);

    return 0;
}
