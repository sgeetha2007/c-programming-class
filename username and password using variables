#include <stdio.h>
#include <string.h>

struct Login {
    char username[20];
    char password[20];
};

int main() {
    struct Login l;
    char user[20], pass[20];

    strcpy(l.username, "doss");
    strcpy(l.password, "7563");

    printf("Enter username: ");
    scanf("%s", user);

    printf("Enter password: ");
    scanf("%s", pass);

    if(strcmp(user, l.username) == 0 && strcmp(pass, l.password) == 0)
        printf("Login Successful");
    else
        printf("Invalid Username or Password");

    return 0;
}
